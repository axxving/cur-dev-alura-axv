---
interface Props {
  titulo: string;
  parrafo: string;
  href: string;
}
const astroProps = Astro.props as Props;
const { titulo, parrafo, href } = astroProps;
---


<div class="col-md-4 mb-4">
  <div class="card">
    <div class="card-body">
      <h5 class="card-title">{titulo}</h5>
      <p class="card-text">
        {parrafo}
      </p>
      <a href={href} class="btn btn-outline-info">Ir al Conversor</a>
    </div>
  </div>
</div>
